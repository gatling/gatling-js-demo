{
  "name": "gatling-ts-demo",
  "version": "3.11.3",
  "private": true,
  "type": "module",
  "main": "target/bundle",
  "dependencies": {
    "@gatling.io/core": "3.11.3",
    "@gatling.io/http": "3.11.3"
  },
  "devDependencies": {
    "@gatling.io/cli": "3.11.3",
    "prettier": "3.2.5",
    "rimraf": "5.0.7",
    "typescript": "5.4.5"
  },
  "scripts": {
    "clean": "rimraf target results",
    "format": "prettier --write **/*.ts",
    "check": "tsc --noEmit",
    "build": "tsc --noEmit && gatling build --typescript",
    "start": "tsc --noEmit && gatling run --typescript --simulation computerdatabase",
    "recorder": "gatling recorder --typescript"
  }
}
