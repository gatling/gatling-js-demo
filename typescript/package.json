{
  "name": "gatling-ts-demo",
  "version": "3.11.6",
  "private": true,
  "type": "module",
  "main": "target/bundle",
  "dependencies": {
    "@gatling.io/core": "3.11.6",
    "@gatling.io/http": "3.11.6"
  },
  "devDependencies": {
    "@gatling.io/cli": "3.11.6",
    "prettier": "3.3.2",
    "rimraf": "6.0.0",
    "typescript": "5.4.5"
  },
  "scripts": {
    "clean": "rimraf target",
    "format": "prettier --write \"**/*.ts\"",
    "check": "tsc --noEmit",
    "build": "tsc --noEmit && gatling build --typescript",
    "recorder": "gatling recorder --typescript",
    "computerdatabase": "tsc --noEmit && gatling run --typescript --simulation computerdatabase"
  }
}
